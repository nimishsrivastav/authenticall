# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Environment
NODE_ENV=development

# API Configuration
VITE_GEMINI_MODEL=gemini-3-flash-preview
VITE_API_TIMEOUT=30000
VITE_MAX_RETRIES=3

# Feature Flags
VITE_ENABLE_TELEMETRY=false
VITE_ENABLE_ADVANCED_FEATURES=true
VITE_ENABLE_DEBUG_MODE=false

# Performance
VITE_FRAME_CAPTURE_FPS=1
VITE_AUDIO_CHUNK_DURATION=5000
VITE_MAX_CONCURRENT_REQUESTS=3

# Trust Score Thresholds
VITE_TRUST_THRESHOLD_SAFE=85
VITE_TRUST_THRESHOLD_CAUTION=50
VITE_TRUST_THRESHOLD_DANGER=0

# Notification Settings
VITE_ENABLE_NOTIFICATIONS=true
VITE_NOTIFICATION_SOUND=true

# Storage
VITE_MAX_HISTORY_ITEMS=100
VITE_CACHE_DURATION=3600000

# Analytics (Optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=
